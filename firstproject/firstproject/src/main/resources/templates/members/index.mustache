{{>layouts/header}}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">EMAIL</th>
            <th scope="col">PASSWORD</th>
        </tr>
    </thead>
    <tbody>
        {{#memberList}}
        <tr>
            <th>{{id}}</th>
            <th><a href="/members/{{id}}">{{email}}</a></th>
            <th>{{password}}</th>
        </tr>
        {{/memberList}}
    </tbody>
</table>
<a href="/members/new" class="btn btn-primary">신규등록</a>

{{>layouts/footer}}