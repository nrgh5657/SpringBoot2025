{{>layouts/header}}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">EMAIL</th>
            <th scope="col">PASSWORD</th>
        </tr>
    </thead>
    <tbody>
        {{#member}}
        <tr>
            <th>{{id}}</th>
            <th>{{email}}</th>
            <th>{{password}}</th>
        </tr>
        {{/member}}
    </tbody>

</table>
<a href="/members/{{id}}/edit" class="btn btn-primary">수정</a>
<a href="/members/{{id}}/delete" class="btn btn-danger">삭제</a>

<a href="/members" class="btn btn-secondary">목록</a>

{{>layouts/footer}}